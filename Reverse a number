using System;
class HelloWorld {
  static void Main() {
    Console.WriteLine("Enter the Input: ");
    int num = Convert.ToInt32(Console.ReadLine());
    int revnum = 0;
    int tempnum = 0;
    while(num!=0){ 
        tempnum = num%10;
        revnum = revnum*10 + tempnum;
        num = num/10;
    }
    Console.WriteLine(revnum);
  }
}
