using System;

class Program{
  static void Main(){
    Console.WriteLine("Enter a number: ");
    int input = int.Parse(Console.ReadLine());
    for(int j = 1;j<=input;j++){
      if(prime(j)){
        Console.WriteLine(j);
      }
    }
  }
  static bool prime(int n){
    if(n <= 1)
    {
      return false;
    }
    for(int i = 2;i<=Math.Sqrt(n);i++){
      if(n%i == 0){
        return false;
      }
    }
    return true;
  }
}
