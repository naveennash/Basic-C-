using System;
using System.Linq;

class Program{
  static void Main(){
    Console.WriteLine("Enter a string:");
    string inputa = Console.ReadLine();
    string[] input = inputa.Split(" ");
    string output="";
    string outputb="";
    for(int i = 0;i<input.Length;i++)
    {
      output = output + char.ToUpper(input[i][0])+input[i].Substring(1)+" ";//Title
      outputb = outputb + char.ToUpper(input[i][0])+input[i].Substring(1);//Pascal
    }
    Console.WriteLine(output);
    Console.WriteLine(outputb);
  }
}
