using System;

class Program{
  static void Main(){
    Console.WriteLine("Enter a string:");
    string input = Console.ReadLine();
    int count = 0;
    string output = " ";
    for(int i = 0;i<input.Length;i++)
    {
      bool IsDuplicate = false;
      for(int j = 0;j<output.Length;j++)
      {
        if(input[i] == output[j])
        {
          IsDuplicate = true;
          break;
        }
      }
      if(!IsDuplicate)
      {
        output = output+input[i];
        count++;
      }
    }
    Console.WriteLine(output);
  }
}
